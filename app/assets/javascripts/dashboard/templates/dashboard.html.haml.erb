                .form-group.has-feedback
                    %input.search-form--search.form-control{ type: 'text', placeholder: 'Search for a scanned fossil specimen', ng: { model: 'form.name', keypress: 'quickSearch($event)' } }
                    %span.iconic.iconic-md.iconic-magnifying-glass.form-control-feedback{ aria: { hidden: 'true' }, title: 'Search', ng: { click: 'search()' } }
                .search-form--advanced-toggle{ ng: { click: 'showAdvanced()' } } Advanced Filters
                .search-form--advanced
                    .search-form--advanced-body
                        %form.form-horizontal
                            .form-group
                                %label.control-label.col-xs-3 Specimen Id
                                .col-xs-9
                                    %input.form-control{ type: 'text', placeholder: 'AMNH-1092929', ng: { model: 'form.name' } }
                            .form-group
                                %label.control-label.col-xs-3 Scientific Name
                                .col-xs-9
                                    %input.form-control{ type: 'text', placeholder: 'Acrocanthosaurus atokensis', ng: { model: 'form.scientific_name' } }
                            .form-group
                                %label.control-label.col-xs-3 Common Name
                                .col-xs-9
                                    %input.form-control{ type: 'text', placeholder: 'Theropod Dinosaur', ng: { model: 'form.common_name' } }
                            .form-group
                                %label.control-label.col-xs-3 Bone Type
                                .col-xs-9
                                    %input.form-control{ type: 'text', placeholder: 'Skull', ng: { model: 'form.bone_type' } }
                            .form-group
                                %label.control-label.col-xs-3 Fossil Group
                                .col-xs-9
                                    %input.form-control{ type: 'text', placeholder: 'Non-avian Dinosaur', ng: { model: 'form.fossil_group' } }
                            .form-group
                                %label.control-label.col-xs-3 Museum
                                .col-xs-9
                                    %input.form-control{ type: 'text', placeholder: 'American Museum of Natural History', ng: { model: 'form.museum' } }
                            .form-group
                                %label.control-label.col-xs-3 Author
                                .col-xs-9
                                    %input.form-control{ type: 'text', placeholder: 'Dr. Amy Balano' , ng: { model: 'form.author' } }

                            .form-group
                                %label.control-label.control-check-label.col-xs-3{ for: 'has-render' } Has Rendering
                                .col-xs-9
                                    .checkbox
                                        %input{ id: 'has-render', type: 'checkbox' }
                                        %label{ for: 'has-render' } &nbsp;
                            %button.btn.btn-primary{ ng: { click: 'filter()' } } Apply Filters

%section.specimens
    %section.specimens--list{ ng: { class: "{ 'has-details': isDetailLoaded }" } }
        .container{ ng: { class: "{ 'is-loaded': isDotsLoaded, 'is-unloaded': isDetailLoaded }" } }
            .specimens--loader-dots
                .specimens--loader-dot{ ng: { repeat: 'dot in loaderDots track by $index', class: "{ 'is-loaded': dot, 'is-active': dot && $last }" } }

        .container{ ng: { class: "{ 'is-loaded': isListLoaded, 'is-unloaded': isDetailLoaded }" } }
            .specimens--header
                Explore some
                %span.specimens--header-count {{ totalSpecimens | number }}
                fossil scans

            .table--header.row
                .col-xs-2 Specimen ID
                .col-xs-3 Scientific Name
                .col-xs-2 Common Name
                .col-xs-2 Bone Type
                .col-xs-2 Fossil Group
                .col-xs-1 Render

            %div{ ng: { if: 'loadData' } }
                .table--body{ infinite_scroll: 'loadMore()', infinite_scroll_distance: '.5' }
                    .table--row.specimen.row{ ng: { repeat: 'specimen in specimens', click: 'openDetails(specimen)', class: "{ 'is-hidden': !specimen.selected && rowSelected }" } }
                        .col-xs-2
                            {{ specimen.name }}
                        .col-xs-3
                            %i {{ specimen.scientific_name }}
                        .col-xs-2
                            {{ specimen.common_name }}
                        .col-xs-2
                            {{ specimen.bone_type }}
                        .col-xs-2
                            {{ specimen.fossil_group }}
                        .col-xs-1
                            STL

        .specimens--detail.container{ ng: { if: 'selectedSpecimen', class: "{ 'is-loaded': isDetailLoaded }" } }
            .specimens--detail-back{ ng: { click: 'search()' } }
                %span.iconic.iconic-chevron-left
                Back to search results

            .specimens--detail-summary.row
                .col-xs-2
                    %img.specimens--detail-thumbnail{ src: '<%= image_url('preview.png') %>' }
                .col-xs-8
                    .specimens--detail-name
                        %span.specimens--detail-tag {{ selectedSpecimen.name }}
                        .specimens--detail-name-header
                            %em {{ selectedSpecimen.scientific_name }},
                            {{ selectedSpecimen.bone_type }}
                        .specimens--detail-name-subheader {{ selectedSpecimen.common_name }}, {{ selectedSpecimen.fossil_group }}
                .col-xs-2
                    .specimens--detail-download
                        %a.btn.btn-primary{ href: '/Zanabazar.stl.zip', target: '_blank' } Download

            .metadata.row
                .col-xs-6
                    %ul.metadata__list
                        %li.metadata__item-icon
                            %span.iconic.iconic-settings
                            %span.metadata__property Voxel Size
                            %span.metadata__value 2&mu;m
                        %li.metadata__item
                            %span.metadata__property Number of images
                            %span.metadata__value {{ selectedSpecimen.calib_num_image }}
                        %li.metadata__item
                            %span.metadata__property Averaging
                            %span.metadata__value {{ selectedSpecimen.calib_averaging }}
                        %li.metadata__item
                            %span.metadata__property Skip
                            %span.metadata__value {{ selectedSpecimen.calib_skip }}
                        %li.metadata__item
                            %span.metadata__property Timing Value
                            %span.metadata__value {{ selectedSpecimen.detector_timing_value }}
                        %li.metadata__item
                            %span.metadata__property Current
                            %span.metadata__value {{ selectedSpecimen.xray_current }}
                        %li.metadata__item
                            %span.metadata__property KiloVolts
                            %span.metadata__value {{ selectedSpecimen.xray_voltage }}
                .col-xs-6
                    %ul.metadata__list
                        %li.metadata__item-icon
                            %span.iconic.iconic-clock
                            %span.metadata__property Date
                            %span.metadata__value {{ selectedSpecimen.created_at }}
                        %li.metadata__item-icon
                            %span.iconic.iconic-person-genderless
                            %span.metadata__property Author
                            %span.metadata__value {{ selectedSpecimen.author }}
                        %li.metadata__item-icon
                            %span.iconic.iconic-map
                            %span.metadata__property Museum
                            %span.metadata__value {{ selectedSpecimen.museum }}

            .figure.row
                .col-xs-6
                    #spriteslider.spriteslider
                        #spritetarget{ style: '{{ selectedSpecimen.sprite }}' } }
                        .spriteslider--help Drag left or right to view
                .col-xs-6
                    %h2 Scans
                    .figure__listing
                        .figure__listing-item.figure__listing-item--active Coronal
                        .figure__listing-item Horizontal
                        .figure__listing-item Sagital

            .figure.row
                .col-xs-6
                    %iframe#api-frame
                .col-xs-6
                    %h2 Renders
                    .figure__listing
                        .figure__listing-item.figure__listing-item--active {{ selectedSpecimen.stls[0] }}
                        .figure__listing-item {{ selectedSpecimen.stls[1] }}
                        .figure__listing-item {{ selectedSpecimen.stls[2] }}
